{
  "extends": "../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "page",
      "kebab-case"
    ],
    "whitespace": false,
    "prefer-const": false,
    "no-trailing-whitespace": false,
    "comment-format": false,
    "triple-equals": false,
    "one-line": false
  }
}
